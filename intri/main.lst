    66                                  ; PROCEDURE:    unpack
    67                                  ; IN:           edx:eax: qword
    68                                  ; OUT:          rax:qword
    69                                  ; MODIFIES:     rax, rdx
    70                                  ; CALLS:        
    71                                  ; DETAILS:      
    72                                  %macro unpack 0
    73                                    shl rdx, 32 ; edx -> left half of rdx
    74                                    ; and rax, 0FFFF_FFFFH ; zero left half of rax
    75                                    ; zero left half of rax
    76                                    shl rax, 32
    77                                    shr rax, 32
    78                                    add rax, rdx ; whole number in rdx now
    79                                  %endmacro
